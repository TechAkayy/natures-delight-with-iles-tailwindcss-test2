    "prettier": "^2.8.4",
    "rehype-external-links": "^2.1.0",
    "svelte": "^3.58.0",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0",
    "unocss": "^0.51.4",
    "vite-plugin-vue-devtools": "^0.0.22",
    "vue-tsc": "^1.2"
