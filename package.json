  "engines": {
    "node": "^14.18 || >= 16.0.0"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.51",
    "@islands/excerpt": "^0.9.0",
    "@islands/feed": "^0.8.1",
    "@islands/headings": "^0.8.2",
    "@islands/prism": "^0.8.0",
    "@nuxt/eslint-config": "^0.1.1",
    "@pinegrow/iles-module": "^3.0.0-beta.122",
    "@pinegrow/tailwindcss-plugin": "^3.0.0-beta.68",
    "@preact/preset-vite": "^2.5.0",
    "@sveltejs/vite-plugin-svelte": "^2.0.4",
